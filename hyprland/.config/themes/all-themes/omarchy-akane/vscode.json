  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#574F72",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#F4B999"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#FA7E75"
      }
    },
    {
      "scope": ["constant.other"],
      "settings": {
        "foreground": "#C85670"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#F4B999"
      }
    },
    {
      "scope": ["variable.parameter", "variable.other.readwrite"],
      "settings": {
        "foreground": "#BE6F76"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#C85670",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#9279AA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator", "keyword.operator.logical"],
      "settings": {
        "foreground": "#F4B999"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#9279AA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#BE6F76"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#9279AA"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#BE6F76"
      }
    },
    {
      "scope": ["support.constant", "constant.language.boolean"],
      "settings": {
        "foreground": "#FA7E75"
      }
    },
    {
      "scope": ["meta.property-name", "entity.name.property"],
      "settings": {
        "foreground": "#BE6F76"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#574F72"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#D2495B",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FA7E75",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9279AA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FA7E75",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
